package test1;

import org.junit.Test;

public class TestCaseSum {
	
	@BeforeClass
	public void beforeAllTests() {
		System.out.println("---Execute before all tests");
	}

	@Before
	public void beforeTest() {
		System.out.println("---Execute before every Test");
	}

	@Test
	public void testMySum() {
		Assert.assertEquals(4, test1.Test.sum(2, 2));
		System.out.println("Hey! First test got passed.");
	}

	@Test
	public void testMySum2() {
		Assert.assertEquals(4, test1.Test.sum(2, 2));
		System.out.println("Hey! First test got passed.");
	}
	
	@After
	public void afterTest() {
		
	}
	
	@AfterClass
	public void afterAllTests() {
		System.out.println("---Execute after all tests");
	}
}
